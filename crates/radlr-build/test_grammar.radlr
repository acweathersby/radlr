IGNORE { c:sp c:nl }

<> statement > e            :ast { t_Stmt, v:$1 }

<> e > e "+"{1} e{1}        :ast { t_Sum, l:$1, r:$3 }
     | e "*"{2} e{2}        :ast { t_Mul, l:$1, r:$3 }
     | term

<> term > tk:num            :ast { t_Num, v:u32($1) }
        | "(" e ")"         :ast { t_Paren, exp:$2 }

<> num > c:num(+)


