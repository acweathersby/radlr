NAME token

<> string > tk:string_tok

<> quote > tk:quote_tok

<> id > tk:id_tok

<> int > tk:int_tok

<> id_tok > ( "-" | "_" | c:id ) ( c:id | "_" | "-" | c:num )(*)

<> string_tok > "\"" ( c:id | c:num | c:nl | c:sym | c:sp | escaped )(*) "\""

<> quote_tok > "\'" ( c:id | c:num | c:nl | c:sym | c:sp | escaped )(*) "\'"

<> escaped > "\\" ( c:id | c:num | c:nl | c:sym | c:sp )

<> int_tok > c:num(+)