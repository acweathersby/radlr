{
  "name": "radlr_grammar",
  "displayName": "Radlr Grammar",
  "description": "Syntax highlighting for Radlr grammar files [*.radlr]",
  "publisher": "candle-smith",
  "version": "1.0.4",
  "engines": {
    "vscode": "^1.22.0"
  },
  "scripts": {
    "local.install": "vsce package && code --install-extension ./*.vsix"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "radlr-grammar",
        "aliases": [
          "Radlr Grammar",
          "radlr-grammar"
        ],
        "extensions": [
          "radlr",
          ".radlr"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "radlr-grammar",
        "scopeName": "source.radlr",
        "path": "./syntaxes/radlr-grammar.tmLanguage.json"
      }
    ]
  },
  "repository": {
    "url": "github.com/acweathersby/radlr/radlr.git/dev/src/grammar/highlighting"
  }
}