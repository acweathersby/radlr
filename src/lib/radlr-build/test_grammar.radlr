IGNORE {c:sp c:nl}

<> json 

  > entry "test"? :ast { t_JSON, body: $1, test:$2 }

<> entry > obj | array
  
<> obj 
  
  > "{" key_val(+",") "}" :ast { t_Object, values: $2 }

<> array
  
  > "[" val(+",") "]" :ast { t_Array, values: $2 }


<> string 
  
  > "\""  ( c:id | c:sp |  c:num | c:sym )(*)  "\""


<> key_val 

  > key ":" val :ast map($1, $3)


<> key 

  > tk:string :ast str(tok<1,1>)


<> val 

  > tk:string :ast str(tok<1,1>)
  | c:num     :ast f64($1)
  | obj
  | array


<> string 
  
  > "\""  ( c:id | c:sp |  c:num | c:sym )(*)  "\""
 

