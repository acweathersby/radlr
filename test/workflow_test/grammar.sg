IGNORE { c:sp c:nl }

  <> statement > e            :ast { t_Result, v:$1 }
  
  <> e > e "+"{1} e{2}        :ast $1 + $3  
       | e "*"{3} e{4}        :ast $1 * $3 
       | term                   
  
  <> term > tk:num            :ast f64($1)  
          | "(" e ")"         :ast $2      
          
  <> num > c:num(+)