	"compilerOptions": {
		"allowJs": true,
		"alwaysStrict": true,
		"checkJs": false,
		"esModuleInterop": true,
		"moduleResolution": "node",
		"skipLibCheck": false,
		"sourceMap": true,
		"strict": true,
		"target": "ESNext",
		"outDir": "./static/",
		"baseUrl": ".",
		"paths": {
			"*": [
				"*"
			]
